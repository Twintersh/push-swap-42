{
"version": "0.2.0",
"configurations": [
	{
		"name": "DEBUG_PUSHSWAP",
		"type": "cppdbg",
		"preLaunchTask": "DEBUG",
		"request": "launch",
		"program": "${fileDirname}/push_swap",
			"args": ["4 000000000000000000000000000000000000000000000000000000002"],
		"stopAtEntry": false,
		"cwd": "${fileDirname}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
			{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			},
			{
				"description": "Set Disassembly Flavor to Intel",
				"text": "-gdb-set disassembly-flavor intel",
				"ignoreFailures": true
			}
		]
	},
]
}